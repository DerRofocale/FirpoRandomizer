<Application
    x:Class="FirpoRandomizer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:FirpoRandomizer"
    StartupUri="Views/MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Designs/IconsDictionary.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Designs/ColorsDictionary.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Designs/FontsDictionary.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Designs/ButtonsDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
